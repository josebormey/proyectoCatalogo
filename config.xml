<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jose.catalogo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CatalogoMultimedia</name>
    <description>Catálogo multimedia con imágenes, videos y textos interactivos</description>
    <author email="jose@example.com" href="https://github.com/josebormey">Jose Bormey</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="22" />
        <preference name="android-targetSdkVersion" value="33" />
    </platform>
</widget>
